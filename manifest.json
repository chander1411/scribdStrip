{
    "manifest_version": 2,
    "name": "Scribd pages reveal",
    "version": "1.0",
    "content_scripts": [
        {
            "matches": [
                "http://www.scribd.com/*",
                "https://www.scribd.com/*",
                "https://scribd.com/*",
                "http://scribd.com/*"
            ],
            "js": ["content.js"]
        }
    ],
    "background": {
        "scripts": ["background.js"]
    },
    "browser_action": {
        "default_icon": "sLogo.png"
    }
}