{
    "manifest_version": 2,
    "name": "Scribd pages reveal",
    "version": "0.1",
    "content_scripts": [
        {
            "matches": [
                "http://github.com/*",
                "https://github.com/*"
            ],
            "js": ["content.js"]
        }
    ],
    "background": {
        "scripts": ["background.js"]
    },
    "browser_action": {
        "default_icon": "sLogo.png"
    }
}